set hls
set nrformats=
set nu
set history=200
set smartindent
set tabstop=4
set shiftwidth=4
set expandtab
set softtabstop=4
set incsearch
inoremap ( ()<ESC>i
inoremap [ []<ESC>i
inoremap { {<CR>}<ESC>kA<CR>
inoremap < <><ESC>i
inoremap ' ''<ESC>i
inoremap " ""<ESC>i
noremap <Up> <Nop>
noremap <Down> <Nop>
noremap <Left> <Nop>
noremap <Right> <Nop>
